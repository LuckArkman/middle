{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "orchestrator-route": {
        "ClusterId": "orchestrator-cluster",
        "Match": {
          "Path": "/orchestrator/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "{**catch-all}" }
        ]
      },
      "container-manager-route": {
        "ClusterId": "container-manager-cluster",
        "Match": {
          "Path": "/container-manager/{**catch-all}"
        },
        "Transforms": [
          { "PathPattern": "{**catch-all}" }
        ]
      }
    },
    "Clusters": {
      "orchestrator-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5001" // Assumindo que o Orchestrator rodará na porta 5001
          }
        }
      },
      "container-manager-cluster": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5002" // Assumindo que o ContainerManager rodará na porta 5002
          }
        }
      }
    }
  }
}